import java.util.Scanner;

public class ConceitoNota {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        // Solicita a nota ao usu치rio
        System.out.print("Digite uma nota de 0 a 10: ");
        int nota = scanner.nextInt();
        
        // Verifica se a nota est치 dentro do intervalo v치lido
        if (nota < 0 || nota > 10) {
            System.out.println("Nota inv치lida.");
        } else {
            // Determina o conceito com switch-case
            String conceito;
            switch (nota) {
                case 9: case 10:
                    conceito = "A";
                    break;
                case 7: case 8:
                    conceito = "B";
                    break;
                case 5: case 6:
                    conceito = "C";
                    break;
                case 3: case 4:
                    conceito = "D";
                    break;
                default:
                    conceito = "E";
            }
            System.out.println("Conceito: " + conceito);
        }
        
        scanner.close();
    }
}
